{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/posts",
      "dest": "/backend/server.js"
    },
    {
      "src": "/posts/:id",
      "dest": "/backend/server.js"
    },
    {
      "src": "/backend/images/(.*)",
      "dest": "/public/images/$1"
    },
    {
      "src": "/.*",
      "dest": "/backend/server.js"
    }
  ]
}
